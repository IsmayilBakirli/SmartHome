            "type": "string"
          },
          "userName": {
            "maxLength": 20,
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "DeviceCreateDto": {
        "required": [
          "categoryId",
          "locationId",
          "name",
          "powerConsumptionWatts"
        ],
        "type": "object",
        "properties": {
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "powerConsumptionWatts": {
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "locationId": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "categoryId": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "cpuUsage": {
            "maximum": 100,
            "minimum": 0,
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "ramUsage": {
            "maximum": 100,
            "minimum": 0,
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "temperature": {
            "maximum": 100,
            "minimum": -100,
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "energyConsumption": {
            "minimum": 0,
            "type": "number",
            "format": "double",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "DeviceStatusDto": {
        "type": "object",
        "properties": {
          "isOnline": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "DeviceUpdateDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "powerConsumptionWatts": {
            "type": "number",
            "format": "double"
          },
          "locationId": {
            "type": "integer",
            "format": "int32"
          },
          "categoryId": {
            "type": "integer",
            "format": "int32"
          },
          "cpuUsage": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "ramUsage": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "temperature": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "energyConsumption": {
            "type": "number",
            "format": "double",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LocationCreateDto": {
        "required": [
          "floor",
          "name",
          "zone"
        ],
        "type": "object",
        "properties": {
          "name": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "zone": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "floor": {
            "maximum": 100,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          },
          "description": {
            "maxLength": 255,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SensorDataCreateDto": {
        "required": [
          "readingType",
          "unit",
